{
  "name": "LinkedIn Profile Extension",
  "description": "An extension to open linkedin profiles in chrome tabs",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "activeTab",
    "tabs",
    "scripting"
  ],
  "host_permissions": [
    "*://*.linkedin.com/*"
  ],
  "action": {
    "default_popup": "index.html",
    "default_icon": "ext-icon.png"
  }
}